@model BryceFamily.Web.MVC.Models.Person

@{
    var unions = Model.Unions.Count();
}

@switch (unions)
{
    case 0:
    case 1:
        Html.RenderPartial("DescendantOneUnionFamilyTreeNode", Model);
        break;
    default:
        Html.RenderPartial("DescendantTwoUnionFamilyTreeNode", Model);
        break;

}
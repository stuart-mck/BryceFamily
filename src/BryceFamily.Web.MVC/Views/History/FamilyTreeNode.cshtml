@model BryceFamily.Web.MVC.Models.Person

@{
    var unions = Model.Unions.Count();
}

@switch (unions)
{
    case 0:
        Html.RenderPartial("NoUnionFamilyTreeNode", Model);
        break;
    case 1:
        Html.RenderPartial("OneUnionFamilyTreeNode", Model);
        break;
    default:
        Html.RenderPartial("TwoUnionFamilyTreeNode", Model);
        break;

}
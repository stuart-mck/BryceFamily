@model IEnumerable<BryceFamily.Web.MVC.Models.AccountViewModels.RoleManagerViewModel>

<h2>Users</h2>

<form asp-controller="Account" asp-action="RoleManager">
    <table class="table">
        <thead>
            <tr>
                <th>Username</th>
                <th>Read Only User</th>
                <th>Clan Manager / Editor</th>
                <th>Super User</th>
            </tr>
        </thead>
        <tbody>
            @Html.EditorForModel()

            
        </tbody>
    </table>
    <button type="submit" class="btn btn-primary">Save Changes</button>
</form>


